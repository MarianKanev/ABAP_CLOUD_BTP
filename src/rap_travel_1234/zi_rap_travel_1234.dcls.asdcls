@EndUserText.label: 'Access Control for ZI_TRAVEL_ARC'
@MappingRole: true
define role ZI_RAP_TRAVEL_1234 {
    grant
        select
            on
                ZI_TRAVEL_ARC
                    where
                        CurrencyCode = 'EUR'
                        and ( travelstatus ) = aspect pfcg_auth( ZOtravstat , zotravstat , ACTVT =  '03' )
                        or true
                        ;
                        
}